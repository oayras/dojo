{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["github.com"],
      "enabled": true
    },
    {
      "matchFileNames": [".github/**"],
      "groupName": "github-actions"
    }
  ]
}
